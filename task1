#include<iostream>
#include<cmath>

using namespace std;
int main()
{
	double e = 1.0;
	double r = 1.0;
	int k = 0;
	int p = 0;
	while ((e / 2 + 1 != 1) || (1 + e) == 1)
	{
		e = e / 2;
		k++;
	}
	while (r < INFINITY)
	{
		r = r * 2;
		p++;
	}
	//cout << e;
	cout << endl;
	cout << k << endl;
	cout << p;

	//printf("%a", e);
	system("pause");
}
